{
  "name": "project-09-data-driven-testing",
  "version": "1.0.0",
  "description": "Playwright TypeScript test suite for data-driven testing (CSV, JSON)",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:csv": "playwright test --grep @csv",
    "test:json": "playwright test --grep @json",
    "test:data-driven": "playwright test --grep @data_driven"
  },
  "keywords": [
    "playwright",
    "typescript",
    "test-automation",
    "data-driven-testing",
    "csv",
    "json",
    "e2e-testing"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}

