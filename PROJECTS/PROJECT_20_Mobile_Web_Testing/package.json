{
  "name": "project-20-mobile-web-testing",
  "version": "1.0.0",
  "description": "Playwright TypeScript test suite for mobile web and responsive testing",
  "scripts": {
    "test": "playwright test",
    "test:mobile": "playwright test --grep @mobile",
    "test:tablet": "playwright test --grep @tablet",
    "test:desktop": "playwright test --grep @desktop",
    "test:responsive": "playwright test --grep @responsive",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report"
  },
  "keywords": [
    "playwright",
    "typescript",
    "test-automation",
    "mobile-testing",
    "responsive-testing",
    "e2e-testing"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.3"
  }
}
