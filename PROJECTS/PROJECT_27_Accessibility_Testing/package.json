{
  "name": "project-27-accessibility-testing",
  "version": "1.0.0",
  "description": "Playwright TypeScript test suite for accessibility testing",
  "scripts": {
    "test": "playwright test",
    "test:accessibility": "playwright test --grep @accessibility",
    "test:aria": "playwright test --grep @aria",
    "test:keyboard": "playwright test --grep @keyboard",
    "test:focus": "playwright test --grep @focus",
    "test:alt": "playwright test --grep @alt",
    "test:axe": "playwright test --grep @axe",
    "test:smoke": "playwright test --grep @smoke",
